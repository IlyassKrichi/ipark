{% extends 'template.html.twig' %}

{% block title %}Parkings index{% endblock %}
{% block PageTitle %}
    <h1>Parkings index</h1>
{% endblock %}
{% block body %}
    

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Capacite</th>
                <th>Emplacement_geo</th>
                <th>Adresse</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for parking in parkings %}
            <tr>
                <td>{{ parking.id }}</td>
                <td>{{ parking.nom }}</td>
                <td>{{ parking.capacite }}</td>
                <td>{{ parking.emplacementGeo }}</td>
                <td>{{ parking.adresse }}</td>
                <td>
                    <a href="{{ path('app_parking_show', {'id': parking.id}) }}">show</a>
                    <a href="{{ path('app_parking_edit', {'id': parking.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_parking_new') }}">Create new</a>
{% endblock %}
