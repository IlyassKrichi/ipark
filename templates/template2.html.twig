							<label for="inputEmail">Email</label>
							<input type="email" value="{{ last_username }}" name="email" id="inputEmail" class="form-control my-1" autocomplete="email" style="padding-top: 12px; padding-bottom: 12px;" required autofocus>
							<label for="inputPassword">Password</label>
							<input type="password" name="password" id="inputPassword" class="form-control my-1" autocomplete="current-password" style="padding-top: 12px; padding-bottom: 12px;" required>

							<input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">

							<div class="form-check my-2">
								<input class="form-check-input" type="checkbox" name="_remember_me" id="flexCheckDefault">
								<label class="form-check-label" for="flexCheckDefault">
									Remember me
								</label>
								<a style="margin-left: 163px" href="{{ path('app_forgot_password_request') }}">Forgot password?</a>
							</div>
							<div class="modal-footer d-flex justify-content-center">
								<button class="btn btn-lg btn-primary" type="submit">
